    <!-- Website Health Report -->

    <div class="container-fluid webHealth text-center mt-1 pt-2 pl-2 pr-2 mb-5" id="website-health">
        <div class="heading-health text-center">
            <h1 class="display-1 headFont">HOW WE CAN HELP YOU?</h1>
        </div>
        <form action="/contact" method="post" class="m-5" id="contact-form">
            <div class="row mt-5">
                <div class="col-lg-4 offset-lg-1 pt-5">
                    <div class="form-group">
                        <input type="text" name="name" class="form-control whInput" placeholder="Enter Name" required>
                    </div>
                </div>    
            

                <div class="col-lg-4 offset-lg-2 pt-5">
                    <div class="form-group">
                        <input type="email" name="email" class="form-control whInput" placeholder="Enter Email" required>
                    </div>
                </div>    
            </div>



            <div class="row">
                <div class="col-lg-4 offset-lg-1 pt-5">
                    <div class="form-group">
                        <input type="number" name="whatsapp" class="form-control whInput" placeholder="Whatsapp No." required>
                    </div>
                </div>    
            

                <div class="col-lg-4 offset-lg-2 pt-5">
                    <div class="form-group">
                        <input type="number" name="mobile" class="form-control whInput" placeholder="Enter Mobile No." required>
                    </div>
                </div>    
            </div>


            <div class="row">
                <div class="col-lg-4 offset-lg-1 pt-5">
                    <div class="form-group">
                        <input type="text" name="city" class="form-control whInput" placeholder="Enter City" required>
                    </div>
                </div>    
            

                <div class="col-lg-4 offset-lg-2 pt-5">
                    <div class="form-group">
                        <input type="text" name="state" class="form-control whInput" placeholder="Enter State" required>
                    </div>
                </div>    
            </div>


            <div class="row">
                <div class="col-lg-4 offset-lg-1 pt-5">
                    <div class="form-group">
                        <input type="number" name="pin" class="form-control whInput" placeholder="Enter Pin" required>
                    </div>
                </div>    
            

                <div class="col-lg-4 offset-lg-2 pt-5">
                    <div class="form-group">
                        <input type="text" name="country" class="form-control whInput" placeholder="Enter Country" required>
                    </div>
                </div>    
            </div>




            <div class="row">
                <div class="col-lg-8 offset-lg-2 pt-5">
                    <div class="form-group">
                        <textarea name="requirements" class="form-control whInput" placeholder="Any Other Requirements" required></textarea>
                    </div>
                </div>      
            </div>




            <div class="row text-center mt-5">
                <div class="col text-center mt-5">
                    <input type="submit" class="btn btn-success whSubmit" name="submit" value="SUBMIT">
                </div>
            </div>
        </form>
    </div>



    

