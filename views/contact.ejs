    <!-- contact heading -->

    <div class="form-contact">
        <div class="form-contact-heading">
            <h1>tell us about your project</h1>
        </div>
        <div class="contact-form">
            <form action="mailto:i.bhupendr@gmail.com" method="POST" enctype="multipart/form-data">

                <div class="general-info-1 general-details">
                    <input type="text" name="name" class="name" placeholder="Name">
                    <input type="email" name="email" class="email" placeholder="Email">
                    <input type="tel" name="wahtsapp_no" class="whatsapp" placeholder="Whatsapp No.">
                </div>
                
                <div class="general-info-2 general-details">
                    <input type="text" name="company_name" class="company_name" placeholder="Your Company Name">
                    <input type="text" name="website" class="website" placeholder="Company Website">
                    <input type="text" name="position" class="position" placeholder="Position">
                </div>    
                <div class="general-info-3 general-details">
                    <input type="text" name="city" class="city" placeholder="City">
                    <input type="text" name="state" class="state" placeholder="State">
                    <input type="text" name="country" class="country" placeholder="Country">
                </div>
                <div class="general-info-4 general-details">
                    <!-- <input type="range" min="1" max="100" value="50" class="slider" id="myRange" placeholder="Budget"> -->
                <textarea name="requirement" class="requirement" cols="30" rows="10" placeholder="Requirement"></textarea>
                </div>
                <div class="general-submit">
                    <input type="submit" name="submit" value="Submit" class="general-submit-btn">
                </div>
                
            </form>
        </div>
        
    </div>

